{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^[models/]",
    "^[config/]",
    "^[api/]",
    "^[services/]",
    "^[hooks/]",
    "^[components/]",
    "^[assets/]",
    "^[../]",
    "^[./]",
    "(.css)$"
  ],
  "importOrderSortSpecifiers": true
}